E2O <- function (E, O) #
{#
    lower = E/O * exp(-1.96 * sqrt(1/O))#
    upper = E/O * exp(+1.96 * sqrt(1/O))#
#
data.frame(EO=E/O,lower=lower,upper=upper)#
}#
