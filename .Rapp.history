AUCs <- function(outcome, pred){#
	roc_obj <- roc(outcome, pred)#
	ci_result <- as.numeric(ci.auc(roc_obj))#
	data.frame(#
		auc = ci_result[2],#
		lower = ci_result[1],#
		upper = ci_result[3]#
	)#
}#
